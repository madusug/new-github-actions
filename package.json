{
  "name": "my-express-app",
  "version": "1.0.0",
  "description": "An Express.js app with testing and linting",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "mocha tests/*.test.js",
    "lint": "eslint . --config eslint.config.js --ext .js,.mjs",
    "lint:fix": "eslint . --config eslint.config.js --ext .js,.mjs --fix"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.28.0",
    "mocha": "^11.1.0",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "supertest": "^6.2.2",
    "nodemon": "^3.0.2"
  },
  "eslintConfig": {
    "root": true
  }
}

